extends /layout/web
include /utils/mixin

block styles
    +link("/static/dist/css/home.css")

block scripts
    +script("/static/dist/js/home.js")

block body
    .container
        //- 轮播
        h4 轮播carousel
        .carousel.slide(data-ride="carousel", id="carouselExampleIndicators")
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="https://iph.href.lu/500x300" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://iph.href.lu/500x301" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://iph.href.lu/500x302" alt="Third slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        h4 alert警告框
        .alert.alert-primary
            span 我是主要的一个提示
            button.close(data-dismiss="alert")
                span(aria-hidden="true") &times;
        h4 面包屑breadcrumb
        nav(aria-label="breadcrumb")
            ol.breadcrumb
                li.breadcrumb-item.active HOME
                li.breadcrumb-item Libray
        h4 按钮button
        button.btn.btn-primary(type="button") 主按钮

        h4 卡片.card
        .card
            img.card-img-top(src="https://iph.href.lu/500x300")
            .card-body
                h5.card-title card title
                p.card-text ttttttssjkl;jfkajfkzmxc,v,mks;dfka;djfkajdsf
                a.btn.btn-primary Go somewhere

        h4 下拉菜单
        .dropdown
            button.btn.btn-secondary.dropdown-toggle(type="button", data-toggle="dropdown") 下拉啊
            .dropdown-menu
                a.dropdown-item(href="#") Action
                a.dropdown-item(href="#") Action1
                a.dropdown-item(href="#") Action2
        h4 折叠
        a.btn.btn-primary(role="button" href="#collapseDemo1" data-toggle="collapse") a点开展示折叠部分
        button.btn.btn-primary(role="button" data-target="#collapseDemo1" data-toggle="collapse") button点开展示折叠部分
        .collapse(id="collapseDemo1")
            .card.card-body
                p 你好我是折叠部分
        
        h4 表单
        .form
            .form-group
                label Email address
                input.form-control(type="email" placeholder="Enter email")
            .form-group
                label Password
                input.form-control(type="password" placeholder="Enter password")
            button.btn.btn-primary(type="submit") Submit
        h4 输入框组
        .input-group
            .input-group-prepend
                .input-group-text @
            input.form-control(type="text" placeholder="Username")
        
        h4 模态框
        button.btn.btn-primary(type="button", data-toggle="modal", data-target="#modalExample") 打开模态框

        .modal.fade(id="modalExample")
            .modal-dialog(role="document")
                .modal-content
                    .modal-header
                        h5 Modal title
                        button.close(data-dismiss="modal")
                            span &times;
                    .modal-body
                        p Modal body text goes here.
                    .modal-footer
                        button.btn.btn-secondary(data-dismiss="modal") Close
                        button.btn.btn-primary Save changes


